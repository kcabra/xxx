script/source = "extends Node2D

var cheese_scale_factor = 1.1

func _ready():
	$Tween.connect(\"tween_completed\", self, \"tween_ended\")
	$Tween.interpolate_property($x21, \"scale\",
			$x21.scale, ($x21.scale*cheese_scale_factor),
			1.0, Tween.TRANS_SINE, Tween.EASE_IN_OUT)
	$Tween.interpolate_property($x22, \"scale\",
			$x21.scale, ($x21.scale*cheese_scale_factor),
			1.0, Tween.TRANS_SINE, Tween.EASE_IN_OUT, 0.4)
	$Tween.start()

var q1 = false
var q2 = false
func tween_ended(obj, _key):
	match obj.name:
		\"x21\":
			if q1:
				$Tween.interpolate_property($x21, \"scale\",
						$x21.scale, ($x21.scale*cheese_scale_factor),
						1.0, Tween.TRANS_SINE, Tween.EASE_IN_OUT)
				q1 = !q1
			else:
				$Tween.interpolate_property($x21, \"scale\",
						$x21.scale, ($x21.scale*(1/cheese_scale_factor)),
						1.0, Tween.TRANS_SINE, Tween.EASE_IN_OUT)
				q1 = !q1
		\"x22\":
			if q2:
				$Tween.interpolate_property($x22, \"scale\",
						$x22.scale, ($x22.scale*cheese_scale_factor),
						1.0, Tween.TRANS_SINE, Tween.EASE_IN_OUT)
				q2 = !q2
			else:
				$Tween.interpolate_property($x22, \"scale\",
						$x22.scale, ($x22.scale*(1/cheese_scale_factor)),
						1.0, Tween.TRANS_SINE, Tween.EASE_IN_OUT)
				q2 = !q2
	$Tween.start()

func _on_home_pressed():
	get_tree().quit()

func _on_replay_pressed():
	get_tree().change_scene(\"res://scenes/kitchen.tscn\")"

[sub_resource type="GDScript" id=3]
script/source = "extends TextureButton

func _ready():
	connect(\"mouse_entered\", self, \"on_mouse_entered\")
	connect(\"mouse_exited\", self, \"on_mouse_exited\")

func on_mouse_entered():
	var sizechange = 1.2
	self.rect_position -= (rect_size*sizechange - rect_size) / 2
	self.rect_scale *= sizechange

func on_mouse_exited():
	self.rect_position += (rect_size - rect_size*(1/rect_scale.x)) / 2
	self.rect_scale *= (1/rect_scale.x)"

